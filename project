void dot()
{
	cout << endl<<endl << "*ALL TRANSACTIONS ARE CASH ON DELIVERY *";
	 cout<<endl << "NOTE: IF ADDRESS OR NUMBER IS WRONG DELIVERY WILL RETURN TO WAREHOUSE";
	int j = 300000;
    for (int i = 0; i < 40; i ++) {
        usleep(j); // move faster and faster,
        j = (j * 1.0); // so sleep less each time
        
        cout<<". "; // dot will display continues with pause
    }
	
}
void dot2()
{
	int j = 300000;
    for (int i = 0; i < 20; i ++) {
        usleep(j); // move faster and faster,
        j = (j * 1.0); // so sleep less each time
       
        cout<<". "; // dot will display continues with pause
    }
}
void rock()
{
	system("color 6");
	{
    for (int i = 0; i < 50; i ++) 
	 cout<<"\n"; 
    cout<<rocket;
    int j = 300000;
    for (int i = 0; i < 30; i ++) {
        usleep(j); // move faster and faster,
        if(i%2==0)
        {
        	system("color e");
		}
		if(i%2==1)
		{
			system("color 6");
		}
        j = (int)(j * 0.9); // so sleep less each time
        cout<<"\n"; // move rocket a line upward
    }
}
}
void rock2()
{
	{
    for (int i = 0; i < 50; i ++) 
	 cout<<"\n"; 
    cout<<rocket2;
    int j = 300000;
    for (int i = 0; i < 30; i ++) {
        usleep(j); // move faster and faster,
        if(i%2==0)
        {
        	system("color c");
		}
		if(i%2==1)
		{
			system("color 4");
		}
        j = (int)(j * 0.9); // so sleep less each time
        cout<<"\n"; // move rocket a line upward
    }
}
}

